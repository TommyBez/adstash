{
  "name": "adstash-extension",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build --config vite.config.ts && vite build --config vite.config.content.ts",
    "build:content": "vite build --config vite.config.content.ts",
    "build:main": "vite build --config vite.config.ts",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.287",
    "typescript": "^5.7.3",
    "vite": "^6.3.5"
  }
}
